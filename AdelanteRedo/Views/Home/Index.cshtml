                            <form action="index.html" method="post">
                                <div class="input-field">
                                    <input type="text" class="form-control" name="name" placeholder="Your Name" required="">
                                </div>
                                <div class="input-field">
                                    <input type="email" class="form-control" name="email" placeholder="Your Email" required="">
                                </div>
                                <div class="input-field message">
                                    <textarea name="message" class="form-control" placeholder="Your Message" required=""></textarea>
                                </div>
                                @Html.Recaptcha()
                                @Html.ValidationMessage("ReCaptcha")
                                <br />
                                <input type="submit" class="btn btn-blue pull-left" value="SEND MESSAGE" id="msg-submit">
                            </form>
                        </div>
                    </div>
                    @*Google Map Location*@
                    <div class="col-md-6 right">

                        <div class="footer-map wow fadeInDown">

                            <style>
                                #map {
                                    height: 400px;
                                    width: 100%;
                                }
                            </style>
                            <h3>Our Location</h3>
                            <div id="map"></div>

                            <script>

                                function initMap() {
                                    var uluru = { lat: 38.207199, lng: -85.648527 };
                                    var map = new google.maps.Map(document.getElementById('map'), {
                                        zoom: 15,
                                        center: uluru
                                    });
                                    var marker = new google.maps.Marker({
                                        position: uluru,
                                        map: map
                                    });
                                }
                            </script>

                            <script async defer
                                    - src="https://maps.googleapis.com/maps/api/js?key=AIzaSyA2aDMkX0rbpslQ46JyJ-V_dUsYCUdiem8&callback=initMap">

                            </script>
                        </div>
                    </div>

